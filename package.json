    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.1",
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.10",
    "multer": "^1.4.5-lts.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
